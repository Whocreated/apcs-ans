#include <bits/stdc++.h>
using namespace std;
int main(int argc, char** argv) {
    int n,t,num=0;
    while (cin>>n){
    int a[n];
    for (int i=0;i<n;i++)
        cin>>a[i];
    for (int i=1;i<=n-1;i++){
        for (int j=0;j<=(n-1)-i;j++){
            if ((a[j]%10)>(a[j+1]%10)){
                t=a[j];
                a[j]=a[j+1];
                a[j+1]=t;
                num++;
            }
        }
    }
    for (int i=1;i<=n-1;i++){
        for (int j=0;j<=(n-1)-i;j++){
            if ((a[j]%10)==(a[j+1]%10) & a[j]<a[j+1]){
                t=a[j];
                a[j]=a[j+1];
                a[j+1]=t;
            }
        }
    }
    for (int i=0;i<n;i++)
        cout<<a[i]<<" ";
    cout<<endl;
    }    
    return 0;
}
