#include <bits/stdc++.h>

using namespace std;

int main() {
    int h, w, n, r, c, t, x, a[25] = {
        0
    };
    cin >> h >> w >> n;
    while (n--) {
        cin >> r >> c >> t >> x;
        for (int i = 0; i < w; i++) {
            if (abs(i - r) + abs(j - c) <= t) {
                a[i] += x;
            }
        }
    }
}
for (int i = 0; i < h; i++) {
    for (int j = 0; j < w; j++) {
        cout << a[i][j] << " ";
    }
}
cout << endl;
}
